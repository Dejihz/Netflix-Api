{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "definitions": {
        "Film": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "film_id": {
                    "type": "integer"
                },
                "content_id": {
                    "type": "integer"
                },
                "duration": {
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 600
                }
            },
            "required": [
                "content_id",
                "duration"
            ],
            "title": "Film"
        }
    }
}