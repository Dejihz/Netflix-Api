{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "definitions": {
        "Series": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "series_id": {
                    "type": "integer"
                },
                "content_id": {
                    "type": "integer"
                },
                "number_of_seasons": {
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 50
                }
            },
            "required": [
                "content_id",
                "number_of_seasons"
            ],
            "title": "Series"
        }
    }
}